<querys>
    <query id="search">
        select d._id, p.surname, p.given_name, l.address_summary, l.contact_summary from doctor d left join profile p on p.id=d.profile_id left join location l on l.id = d.location_id where d._id in
    </query>

    <query id="getDoctorByDoctorId">
        select * from doctor where _id = ?
    </query>

    <query id="getPrivilegeByDoctorId">
        select * from privilege where doctor_id = ?
    </query>

    <query id="getSpecialtyByDoctorId">
        select * from specialty where doctor_id = ?
    </query>

    <query id="getProfileByProfileId">
